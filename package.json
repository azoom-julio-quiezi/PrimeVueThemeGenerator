{
  "name": "@azoom/primevue-theme-generator",
  "version": "1.0.0",
  "description": "Theme generator for PrimeVue components",
  "main": "src/index.ts",
  "bin": {
    "primevue-theme": "./dist/cli.js"
  },
  "files": [
    "dist",
    "assets"
  ],
  "scripts": {
    "create": "ts-node src/cli.ts create --output ./test",
    "convert-tokens": "ts-node src/cli.ts convert-tokens -i ./tokens/tokens.json -o ./test/themes/theme-tokens.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "rm -rf dist && tsc",
    "prepare": "npm run build && test -f dist/cli.js && chmod +x dist/cli.js"
  },
  "keywords": [
    "primevue",
    "theme",
    "generator"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/commander": "^2.12.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.24",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@primeuix/themes": "1.0.1",
    "commander": "^13.1.0"
  }
}
