{
  "name": "@azoom/primevue-theme-generator",
  "version": "1.0.0",
  "description": "Theme generator for PrimeVue components",
  "main": "src/index.ts",
  "bin": {
    "primevue-theme": "./dist/cli.js"
  },
  "files": [
    "dist",
    "assets",
    "custom-components",
    "custom-composables",
    "nuxt"
  ],
  "scripts": {
    "create-theme-test": "ts-node src/cli.ts create --output ./test",
    "create-theme": "ts-node src/cli.ts create --output .",
    "convert-tokens": "ts-node src/cli.ts convert-tokens -i ./tokens/tokens.json -o ./test/themes/theme-tokens.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "rm -rf dist && tsc",
    "prepare": "pnpm run build && test -f dist/cli.js && chmod +x dist/cli.js",
    "postinstall": "node dist/cli.js create --output ."
  },
  "keywords": [
    "primevue",
    "theme",
    "generator",
    "vue",
    "nuxt"
  ],
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "vue": "^3.0.0",
    "primevue": "^4.0.0",
    "@primevue/nuxt-module": "^4.0.0",
    "@primeuix/themes": "^1.0.0",
    "@azoom/az-icons": "^0.1.0",
    "nuxt": "^3.0.0",
    "sass": "^1.87.0"
  },
  "peerDependenciesMeta": {
    "nuxt": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/commander": "^2.12.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.24",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@primeuix/themes": "1.0.1",
    "commander": "^13.1.0"
  },
  "packageManager": "pnpm@9.9.0+sha512.60c18acd138bff695d339be6ad13f7e936eea6745660d4cc4a776d5247c540d0edee1a563695c183a66eb917ef88f2b4feb1fc25f32a7adcadc7aaf3438e99c1"
}
